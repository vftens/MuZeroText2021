{
  "name": "Atari",
  "algorithm": "ALPHAZERO",
  "architecture": "Atari",
  
  "args": {
    "num_selfplay_iterations": 100,
    "num_episodes": 100,
    "num_gradient_steps": 300,
    "max_episode_moves": 500,
    "max_trial_moves": 500,
    "pitting": true,
    "pitting_trials": 30,
    "pit_acceptance_ratio": 0.55,
    "dirichlet_alpha": 0.3,
    "exploration_fraction": 0.25,
    "max_buffer_size": 200000,
    "num_MCTS_sims": 25,
    "prioritize": false,
    "prioritize_alpha": 1,
    "prioritize_beta": 1,
    "n_steps": 10,
    "c1": 1.25,
    "c2": 19652,
    "gamma": 1,

    "minimum_reward": -1,
    "maximum_reward": 1,

    "checkpoint": "./out/AlphaZeroOut/",
    "load_model": false,
    "load_folder_file": ["./out/AlphaZeroOut/", "latest.pth.tar"],
    "selfplay_buffer_window": 20,

    "temperature_schedule": {
      "method": "stepwise",
      "by_weight_update": false,
      "schedule_points": [[15, 1], [15, 0]]
    }
  },
  
  "net_args": {
    "optimizer": {
      "method": "adam",
      "lr_init": 0.001,
      "momentum": 0.9
    },
    "l2": 1e-4,
    "dropout": 0.0,
    "batch_size": 128,
    "cuda": false,
    "num_channels": 128,
    "num_convs": 1,
    "num_towers": 2,
    "residual_left": 2,
    "residual_right": 0,
    "num_dense": 2,
    "size_dense": 256,
    "activation": "leakyrelu",
    "support_size": 50,
    "observation_length": 5
  }
}
